<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiMaps\ApiMapBundle\Entity\Logindata" table="logindata">
    <unique-constraints>
      <unique-constraint name="logindata_sid_key" columns="sid"/>
    </unique-constraints>
    <id name="sid" type="integer" column="sid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passward" type="text" column="passward" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
